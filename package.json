{
  "name": "clipprs",
  "description": "CLI Personal Relations System",
  "version": "0.0.5",
  "author": "Harrison Broadbent @harrison-broadbent",
  "bin": {
    "clipprs": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/harrison-broadbent/clipprs/issues"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cli-table3": "^0.6.0",
    "clipprs-enquirer": "^2.3.6-0.0.1",
    "lodash": "^4.17.19",
    "lowdb": "^1.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">11.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/harrison-broadbent/clipprs",
  "keywords": [
    "oclif",
    "cli",
    "pkm",
    "knowledge-management"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "clipprs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harrison-broadbent/clipprs.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
